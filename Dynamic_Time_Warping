   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### 3. Dynamic Time Warping: application on real data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "scrolled": false
   },
   "outputs": [],
   "source": [
    "# loop over all dataset rows\n",
    "# and calculate cost distance for each pair\n",
    "cost_id = []\n",
    "for index, row in df.iterrows():\n",
    "    a = convert_string2numbers(row.series_a)\n",
    "    b = convert_string2numbers(row.series_b)\n",
    "    \n",
    "    eucl_distances = eucl_distance_calc(a, b)\n",
    "    accumulated_cost = accumulated_cost_calc(a, b, eucl_distances)\n",
    "    _, cost = path_cost_calc(a, b, accumulated_cost, eucl_distances)\n",
    "    cost_id.append(cost)\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# print results to csv file\n",
    "output = pd.DataFrame()\n",
    "output['id'] = df.id\n",
    "output['distance'] = cost_id\n",
    "output.to_csv(\"requirement3.csv\", index=False, sep=',')\n"
   ]
  }
